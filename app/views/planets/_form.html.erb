<%= form_for @planet do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :image_Url %>
  <%= f.url_field :image %>
  <%= f.label :diameter_km %>
  <%= f.number_field :diameter %>
  <%= f.label :mass_kg %>
  <%= f.number_field :mass %>
  <%= f.label :inhabitable %>
  <%= f.check_box :life %>
  <%= f.submit %>
<% end %>
